<!DOCTYPE html>
<html>
  <head>
    <title>Update User</title>
  </head>
  <body>
    <form method="post">
      <fieldset>
        <legend>Update User</legend>
        <table>
          <tr>
            <td>Username</td>
            <td>
              <input
                type="text"
                name="username"
                value="<%= userlist[0].username %>"
              />
            </td>
          </tr>
          <tr>
            <td>Password</td>
            <td>
              <input
                type="password"
                name="password"
                value="<%= userlist[0].password %>"
              />
            </td>
          </tr>
          <tr>
            <td>Type</td>
            <td>
              <input type="text" name="type" value="<%= userlist[0].type %>" />
            </td>
          </tr>
          <tr>
            <td></td>
            <td><input type="submit" name="submit" value="Update" /></td>
          </tr>
        </table>
      </fieldset>
    </form>
  </body>
</html>
